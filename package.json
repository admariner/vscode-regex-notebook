{
	"name": "regex-notebook",
	"displayName": "regex notebook",
	"description": "Regexper Notebooks",
	"version": "0.0.1",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onNotebook:regexpnb"
	],
	"main": "./dist/extension.js",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"contributes": {
		"notebooks": [
			{
				"type": "regexpnb",
				"displayName": "Regex Notebook",
				"selector": [
					{
						"filenamePattern": "*.regexnb"
					}
				]
			}
		],
		"notebookRenderer": [
			{
				"id": "regexp-renderer",
				"entrypoint": "./dist/regexper-renderer.js",
				"displayName": "Regex Renderer",
				"mimeTypes": [
					"application/x.regexp"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack -c webpack.config.js && tsc -p ./src/extension",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./src/extension",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "14.14.37",
		"@types/vscode-notebook-renderer": "^1.57.8",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"esbuild": "^0.12.6",
		"eslint": "^7.6.0",
		"typescript": "^4.3.2"
	}
}
