{
	"name": "regex-notebook",
	"displayName": "regex notebook",
	"description": "Regexper Notebooks",
	"version": "0.0.1",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onNotebook:regexpnb"
	],
	"main": "./out/extension.js",
	"contributes": {
		"notebooks": [
			{
				"viewType": "regexpnb",
				"displayName": "Regex Notebook",
				"selector": [
					{
						"filenamePattern": "*.regexnb"
					}
				]
			}
		],
		"notebookOutputRenderer": [
			{
				"id": "regexp-renderer",
				"entrypoint": "./dist/regexper.js",
				"displayName": "Regex Renderer",
				"mimeTypes": [
					"application/x.regexp"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack -c webpack.config.js && tsc -p ./src/extension",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./src/extension",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "14.14.37",
		"@types/vscode-notebook-renderer": "^1.57.2",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"canopy": "^0.2.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"lodash": "^4.17.21",
		"mocha": "^8.0.1",
		"snapsvg-cjs": "0.0.6",
		"ts-loader": "^8.0.3",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^5.37.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"@types/node": "^14.14.37"
	}
}
